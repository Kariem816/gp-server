{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "paths": {
            "~/*": [
                "./src/*"
            ]
        },
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "jsx": "react-jsx",
        "moduleResolution": "Bundler"
    },
}